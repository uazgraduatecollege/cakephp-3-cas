    "license": [
      "MIT",
      "Apache-2.0"
    ],
    "type": "cakephp-plugin",
    "authors": [
      {
        "name": "UA Graduate College IT",
        "homepage": "https://grad.arizona.edu/it/",
        "role": "Maintainer"
      },
      {
        "name": "Patrick Barabe",
        "email": "pbarabe@arizona.edu",
        "role": "Developer"
      },
      {
        "name": "Lee Marsh",
        "email": "rmarsh1@arizona.edu",
        "role": "Developer"
      }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "~3.8.0",
        "apereo/phpcas": "~1.4.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
